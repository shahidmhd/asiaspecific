<div class="page-wrapper">

    <!-- Page Content-->
    <div class="page-content-tab">

        <div class="container-fluid">
            <!-- Page-Title -->
            <div class="row">
                <div class="col-sm-12">
                    <div class="page-title-box">

                        <h4 class="page-title">Basic Tables</h4>
                    </div>
                    <!--end page-title-box-->
                </div>
                <!--end col-->
            </div>
            <!-- end page title end breadcrumb -->



            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-header">
                            <h4 class="card-title">Job Applied Datas</h4>

                        </div><!--end card-header-->
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-striped mb-0">
                                    <thead>
                                        <tr>
                                            <th>Name</th>
                                            <th>Email</th>
                                            <th>Phone</th>
                                            <th>Age</th>
                                            <th>Qualification</th>
                                            <th>Speed</th>
                                            <th>English Knowledege</th>
                                            <th>Gender</th>
                                            <th>Job</th>
                                            <th class="text-end">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% if (data.length > 0) { %>
                                            <% data.forEach(item => { %>
                                                <tr>
                                                    <td><%= item.name %></td>
                                                    <td><%= item.email%></td>
                                                    <td><%= item.phone %></td>
                                                    <td> <%= item.age %></td>
                                                    <td><%= item.qualification %></td>
                                                    <td><%= item.speed %></td>
                                                    <td><%= item.englishKnowledge %></td>
                                                    <td><%= item.gender %></td>
                                                    <td><%= item.jobId.category %></td>
                                                    <td></td>
                                                    <td class="text-end">
                                                       <a href="#" class="btn btn-danger" data-bs-target="#modaldemo3_<%= item._id %>" data-bs-toggle="modal">Delete </a>
                                                    </td>
                                                </tr>
                                            <% }); %>
                                        <% } else { %>
                                            <tr>
                                                <td colspan="5" class="text-center">No services available.</td>
                                            </tr>
                                        <% } %>
                                    </tbody>
                                    
                                </table><!--end /table-->
                            </div><!--end /tableresponsive-->
                        </div><!--end card-body-->
                    </div><!--end card-->
                </div> <!-- end col -->


            </div> <!-- end row -->
            
          

            <% for (let i = 0; i < data.length; i++) { %>
                <% let item = data[i]; %>
                <!-- Delete Modal -->
                <div id="modaldemo3_<%= item._id %>" class="modal fade">
                    <div class="modal-dialog" role="document">
                        <div class="modal-content">
                            <div class="modal-body">
                                <div class="modal-header" >
                                    <h5 class="modal-title" id="exampleModalLabel">Confirm</h5>
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                </div>
                                <div class="modal-body">
                                    <p>Are you sure you want to delete?</p>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-secondary" id="close-modal">No</button>
                                    <a href="/admin/deletecontact/<%= item._id %>" class="btn btn-danger" type="button">Yes</a>
                                </div>
                            </div><!-- modal-content -->
                        </div><!-- modal-dialog -->
                    </div>
                </div>
            <% } %>
            <!-- Delete Modal Close -->



        </div><!-- container -->

        <!--Start Rightbar-->

        <!--end Rightbar-->

        <!--Start Footer-->
        <!-- Footer Start -->
    
    </div>
    <!-- end page content -->
</div>
